e(article.createdAt).toISOString().split('T')[0] }}</td>\n          </tr>\n        </tbody>\n      </table> -->\n      <div class=\"w-full flex mt-10 mb-4 border-b\">\n        <div class=\"w-1/4\">Title</div>\n        <div class=\"w-1/4\">Slug</div>\n        <div class=\"w-1/4\">Date</div>\n        <div class=\"w-1/4\">Action</div>\n      </div>\n      <div class=\"w-full flex mb-2\" v-for=\"article in articles\" :key=\"article._id\">\n        <div class=\"w-1/4\">\n          <button @click=\"getArticle(article._id)\">{{ article.title }}</button>\n        </div>\n        <div class=\"w-1/4\">{{ article.slug }}</div>\n        <div class=\"w-1/4\">{{ new Date(article.createdAt).toISOString().split('T')[0] }}</div>\n        <div class=\"w-1/4\">\n          <button @click=\"removeArticle(article._id)\">Remove</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport EventBus from '../eventBus'\nimport axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      articles: []\n    }\n  },\n  props: ['token'],\n  methods: {\n    getArticle(id) {\n      const payload = {\n        id\n      }\n      EventBus.$emit('changePage', { page: 'edit-article' })\n      // setTimeout(() => { EventBus.$emit('getArticle', payload) }, 100)\n      this.$nextTick(() => {\n        EventBus.$emit('getArticle', payload)\n      })\n    },\n    removeArticle(id) {\n      axios({\n        method: 'delete',\n        url: `http://localhost:3000/articles/${id}`,\n        headers: {\n          token: this.token\n        }\n      })\n        .then(({ data }) => {\n          // console.log(data)\n          // this.articles = data\n          this.fetchArticles()\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    },\n    fetchArticles() {\n      axios({\n        method: 'get',\n        url: 'http://localhost:3000/articles',\n        headers: {\n          token: this.token\n        }\n      })\n        .then(({ data }) => {\n          // console.log(data)\n          this.articles = data\n          console.log(this.articles)\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    }\n  },\n  created() {\n    this.fetchArticles()\n  }\n}\n</script>\n\n<style></style>\n"},"lineCount":null}},"error":null,"hash":"663e1ad085d605b6d7c64d78dad3cdb0","cacheData":{"env":{}}}