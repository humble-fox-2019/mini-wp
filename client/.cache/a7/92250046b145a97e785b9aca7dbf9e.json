":"<template>\n  <div class=\"new-box p-4\">\n    <h1 class=\"text-center\">Post New Article</h1>\n    <form @submit.prevent=\"post()\" class=\"mt-4\">\n      <label>Title</label>\n      <input\n        type=\"text\"\n        autocomplete=\"off\"\n        placeholder=\"Input Title\"\n        v-model=\"title\"\n        class=\"form-control\"\n      />\n      <Label class=\"mt-2\">Content</Label>\n      <wysiwyg v-model=\"content\" aria-placeholder=\"Input content\" style=\"color: whitesmoke;\"></wysiwyg>\n      <label class=\"mt-2\">Picture</label>\n      <input\n        type=\"file\"\n        v-on:change=\"handlefileupload($event)\"\n        lang=\"es\"\n        accept=\".jpg\"\n        class=\"form-control\"\n      />\n      <label>Tags</label>\n      <input\n        type=\"text\"\n        autocomplete=\"off\"\n        placeholder=\"Input tag\"\n        v-model=\"tag\"\n        class=\"form-control\"\n      />\n      <div class=\"text-center mt-4\">\n        <button class=\"btn\">Post Article</button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nconst url = `http://localhost:3000`;\nexport default {\n  data() {\n    return {\n      title: \"\",\n      content: \"\",\n      file: \"\",\n      tag: \"\"\n    };\n  },\n  methods: {\n    handlefileupload(event) {\n      let file = event.target.files || event.dataTransfer.files;\n      this.file = file[0];\n    },\n    post() {\n      Swal.showLoading();\n      const token = localStorage.getItem(\"token\");\n      let formData = new FormData();\n      formData.set(\"title\", this.title);\n      formData.set(\"content\", this.content);\n      formData.set(\"file\", this.file);\n      formData.set(\"tag\", this.tag);\n      axios({\n        url: `${url}/articles`,\n        method: \"post\",\n        data: formData,\n        headers: {\n          token\n        }\n      })\n        .then(({ data }) => {\n          this.title = \"\";\n          this.content = \"\";\n          this.file = \"\";\n          this.tag = \"\";\n          Swal.close();\n          Swal.fire({\n            type: \"success\",\n            text: \"Article has been Posted\"\n          });\n        })\n        .catch(err => {\n          console.log(err);\n          Swal.fire({\n            type: \"error\",\n            text: \"Invalid input parameters\"\n          });\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.new-box {\n  /* background-color: grey; */\n  width: 100%;\n}\nh1 {\n  color: gray;\n}\nlabel {\n  font-weight: bolder;\n  font-size: 22px;\n}\nbutton {\n  font-size: 22px;\n  transition: 0.7s all;\n}\nbutton:hover {\n  color: whitesmoke;\n  transition: 0.7s all;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"bddb3fc62d0930fcbe9dd221a299acaa","cacheData":{"env":{}}}